{
    "name": "teamtnt/tntsearch",
    "type": "library",
    "description": "A fully featured full text search engine written in PHP",
    "keywords": [
        "teamtnt",
        "tntsearch"
    ],
    "homepage": "https://github.com/teamtnt/tntsearch",
    "license": "MIT",
    "authors": [
        {
            "name": "Nenad Tičarić",
            "email": "nticaric@gmail.com",
            "homepage": "http://www.tntstudio.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*"
    },
    "autoload": {
        "psr-4": {
            "TeamTNT\\": "src"
        },
        "files": [
            "helper/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "TeamTNT\\TNTSearch\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
